{
    "name": "@parced/ui",
    "description": "A UI framework for Vue.js ",
    "version": "0.0.3",
    "keywords": [
        "parced ui",
        "ui framework",
        "ui library",
        "vue ui",
        "vue3",
        "vue framework",
        "ui"
    ],
    "license": "MIT",
    "author": {
        "name": "Samuel Alev",
        "email": "samuel.alev@protonmail.com"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/parced-ui.umd.js",
    "unpkg": "dist/parced-ui.umd.js",
    "jsdelivr": "dist/parced-ui.umd.js",
    "module": "dist/parced-ui.es.js",
    "types": "dist/types/parced-ui.d.ts",
    "exports": {
        "./dist/parced-ui.css": "./dist/parced-ui.css",
        "./dist/parced-ui.cjs.js": "./dist/parced-ui.cjs.js",
        ".": {
            "import": "./dist/parced-ui.es.js",
            "require": "./dist/parced-ui.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/parced/ui.git"
    },
    "scripts": {
        "build": "npm run clean && npm run build:library && npm run build:types",
        "build:library": "vite build",
        "build:storybook": "build-storybook",
        "build:types": "vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
        "clean": "rimraf dist",
        "dev": "start-storybook -p 6006 --no-open",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "test": "vitest",
        "test:ci": "vitest --run"
    },
    "devDependencies": {
        "@parced/eslint-config": "0.1.0",
        "@storybook/addon-a11y": "6.5.0-alpha.39",
        "@storybook/addon-actions": "6.5.0-alpha.39",
        "@storybook/addon-docs": "6.5.0-alpha.39",
        "@storybook/addon-essentials": "6.5.0-alpha.39",
        "@storybook/addon-links": "6.5.0-alpha.39",
        "@storybook/addons": "6.5.0-alpha.39",
        "@storybook/theming": "6.5.0-alpha.39",
        "@storybook/vue3": "6.5.0-alpha.39",
        "@types/node": "17.0.17",
        "@vitejs/plugin-vue": "2.2.4",
        "@vue/test-utils": "2.0.0-rc.17",
        "autoprefixer": "10.4.4",
        "eslint": "8.9.0",
        "jsdom": "19.0.0",
        "postcss": "8.4.8",
        "prettier": "2.5.1",
        "rimraf": "3.0.2",
        "storybook-builder-vite": "0.1.18",
        "storybook-dark-mode": "1.0.8",
        "tailwindcss": "3.0.23",
        "ts-node": "10.5.0",
        "typescript": "4.5.5",
        "vite": "2.8.6",
        "vitest": "0.3.4",
        "vue": "3.2.31",
        "vue-tsc": "0.31.4"
    },
    "dependencies": {
        "@heroicons/vue": "1.0.5",
        "@parced/fonts": "^0.0.1",
        "focus-trap": "^6.7.3",
        "focus-trap-vue": "^3.2.1",
        "uid": "2.0.0"
    },
    "peerDependencies": {
        "vue": "^3.2.25"
    }
}
